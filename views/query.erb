<table>
  <thead>
    <tr>
      <% @columns.each do |column| %>
        <th><%= column %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @results.each do |row| %>
        <tr>
          <% @columns.each do |col| %>
            <td>
              <%= row[col] %>
            </td>
          <% end %>
        </tr>
    <% end %>
  </tbody>
</table>

