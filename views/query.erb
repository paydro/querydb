<table>
  <thead>
    <tr>
      <% @columns.each do |column| %>
        <th><%= column %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @results.each do |row| %>
        <tr>
          <% row.each_with_index do |val,i| %>
            <td<%= column_class(@columns[i], val) %>><%= h val %></td>
          <% end %>
        </tr>
    <% end %>
  </tbody>
</table>

